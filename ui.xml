<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="resizable">none</property>
    <property name="title" translatable="yes">League Creator</property>
    <child>
      <object class="tk.Frame" id="main_frame">
        <property name="height">600</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.LabelFrame" id="tasks_frame">
            <property name="height">200</property>
            <property name="text" translatable="yes">Tasks</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
            <child>
              <object class="tk.Frame" id="buttons_frame">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">10</property>
                  <property name="pady">10</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                </layout>
                <child>
                  <object class="tk.Button" id="button1">
                    <property name="command" cbtype="simple" type="command">start_account_creation</property>
                    <property name="text" translatable="yes">Start</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="tk.Frame" id="input_frame">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">10</property>
                  <property name="pady">10</property>
                  <property name="row">1</property>
                  <property name="sticky">nsew</property>
                </layout>
                <child>
                  <object class="tk.Label" id="label1">
                    <property name="text" translatable="yes">Amount to create</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Spinbox" id="spinbox1">
                    <property name="increment">-999</property>
                    <property name="textvariable">int:accounts_count</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label2">
                    <property name="text" translatable="yes">Anti Captcha API Key</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Entry" id="captcha_key">
                    <property name="textvariable">string:captcha_key</property>
                    <property name="width">40</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label3">
                    <property name="text" translatable="yes">Region</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Combobox" id="region">
                    <property name="state">readonly</property>
                    <property name="textvariable">string:region</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label4">
                    <property name="text" translatable="yes">Write Format</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Combobox" id="write_format">
                    <property name="state">readonly</property>
                    <property name="textvariable">string:write_format</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Checkbutton" id="checkbutton1">
                    <property name="command" cbtype="simple" type="command">set_is_use_proxies</property>
                    <property name="text" translatable="yes">Use Proxy IP addresses</property>
                    <property name="variable">boolean:is_use_proxies</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">4</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="proxy_file_path_label">
                    <property name="text" translatable="yes">Proxies file path</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">4</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Entry" id="proxies_file_path">
                    <property name="textvariable">string:proxies_file_path</property>
                    <property name="width">45</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Button" id="browse1">
                    <property name="command" cbtype="simple" type="command">set_proxies_path</property>
                    <property name="text" translatable="yes">Browse</property>
                    <layout manager="grid">
                      <property name="column">2</property>
                      <property name="row">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label8">
                    <property name="text" translatable="yes">Email host</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">6</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Entry" id="email_host">
                    <property name="textvariable">string:email_host</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">7</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label9">
                    <property name="text" translatable="yes">Account Write Path</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">6</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Entry" id="account_write_path">
                    <property name="textvariable">string:account_write_path</property>
                    <property name="width">45</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">7</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Button" id="browse2">
                    <property name="command" cbtype="simple" type="command">set_accounts_write_path</property>
                    <property name="text" translatable="yes">Browse</property>
                    <layout manager="grid">
                      <property name="column">2</property>
                      <property name="row">7</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="tk.LabelFrame" id="labelframe1">
                <property name="height">200</property>
                <property name="text" translatable="yes">Stats</property>
                <property name="width">200</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="row">0</property>
                  <property name="rowspan">2</property>
                  <property name="sticky">nsew</property>
                </layout>
                <child>
                  <object class="tk.Label" id="label5">
                    <property name="text" translatable="yes">Captcha Token Used</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">0</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="captcha_usage_count">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">string:captcha_usage_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="padx">15</property>
                      <property name="row">0</property>
                      <property name="sticky">nsew</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label7">
                    <property name="text" translatable="yes">Remaining Accounts</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">1</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label10">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">string:remaining_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label11">
                    <property name="text" translatable="yes">Signed Up Accounts</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">2</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label12">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">string:signed_up_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label6">
                    <property name="text" translatable="yes">Proxy count</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">3</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label13">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">string:proxy_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">3</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label14">
                    <property name="text" translatable="yes">InvalidToken</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">4</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label15">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">int:invalid_token_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">4</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label16">
                    <property name="text" translatable="yes">ValueNotUnique</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">5</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label17">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">int:value_not_unique_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label18">
                    <property name="text" translatable="yes">UnsupportedCountry</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">6</property>
                      <property name="sticky">e</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="label19">
                    <property name="text" translatable="yes">0</property>
                    <property name="textvariable">int:unsupported_country_count</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">6</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.LabelFrame" id="console_frame">
            <property name="text" translatable="yes">Console</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">nsew</property>
            </layout>
            <child>
              <object class="tk.Text" id="console">
                <property name="height">7</property>
                <property name="insertofftime">0</property>
                <property name="width">117</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.LabelFrame" id="status_frame">
            <property name="height">200</property>
            <property name="text" translatable="yes">Status</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
              <property name="sticky">nsew</property>
            </layout>
            <child>
              <object class="tk.Entry" id="status">
                <property name="textvariable">string:status</property>
                <property name="width">150</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">nsew</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="frame1">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">3</property>
              <property name="sticky">nsew</property>
            </layout>
